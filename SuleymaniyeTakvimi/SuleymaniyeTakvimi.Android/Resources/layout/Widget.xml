<?xml version="1.0" encoding="utf-8" ?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="@android:drawable/dialog_holo_light_frame"
    android:id="@+id/widgetBackground"
    android:orientation="horizontal"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/frameLayout"
        android:background="#EFEBE9">
        <GridLayout
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/gridLayout"
            android:rowCount="9"
            android:columnCount="1">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:id="@+id/topLinearLayout">
                <ImageView
                    android:id="@+id/widgetAppIcon"
                    android:src="@drawable/app_logo"
                    android:layout_marginLeft="5dip"
                    android:layout_marginTop="5dip"
                    android:layout_marginRight="-5dip"
                    android:layout_marginBottom="-5dip"
                    android:layout_gravity="center_vertical|center_horizontal"
                    android:baselineAlignBottom="false"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:scaleX="0.5"
                    android:scaleY="0.5"
                    android:contentDescription="@string/app_name"
                    android:pointerIcon="hand" />
                <TextView
                    android:id="@+id/widgetAppName"
                    android:text="@string/app_name"
                    android:textColor="#121212"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:layout_marginLeft="-5dip"
                    android:layout_gravity="center_vertical|center_horizontal"
                    android:textSize="@dimen/abc_text_size_menu_material"
		            android:gravity="left" />
                <TextView
                    android:id="@+id/widgetLastRefreshed"
                    android:textColor="#121212"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:layout_marginLeft="5dip"
                    android:layout_gravity="center_vertical|center_horizontal"
                    android:textSize="@dimen/abc_text_size_caption_material" />
                <ImageView
                    android:id="@+id/widgetRefreshIcon"
                    android:src="@drawable/refresh"
                    android:layout_marginLeft="5dip"
                    android:layout_marginTop="5dip"
                    android:layout_gravity="center_vertical|end"
                    android:baselineAlignBottom="false"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:contentDescription="@string/refresh"
                    android:pointerIcon="hand"/>
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:id="@+id/fecriKazipLinearLayout"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:paddingStart="20dip"
                android:paddingEnd="20dip">
                <TextView
                    android:id="@+id/widgetFecriKazip"
                    android:text="@string/fecri_kazip"
                    android:textColor="#121212"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="5dp"
                    android:gravity="start"
                    android:textSize="@dimen/abc_text_size_medium_material" />
                <TextView
                    android:id="@+id/widgetFecriKazipVakit"
                    android:textColor="#121212"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="5dp"
                    android:gravity="end"
                    android:textSize="@dimen/abc_text_size_medium_material" />
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:id="@+id/fecriSadikLinearLayout"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:paddingStart="20dip"
                android:paddingEnd="20dip">
                <TextView
                    android:id="@+id/widgetFecriSadik"
                    android:text="@string/fecri_sadik"
                    android:textColor="#121212"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="5dp"
                    android:textSize="@dimen/abc_text_size_medium_material" />
                <TextView
                    android:id="@+id/widgetFecriSadikVakit"
                    android:textColor="#121212"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="5dp"
                    android:gravity="end"
                    android:textSize="@dimen/abc_text_size_medium_material" />
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:id="@+id/sabahSonuLinearLayout"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:paddingEnd="20dip"
                android:paddingStart="20dip">
                <TextView
                    android:id="@+id/widgetSabahSonu"
                    android:text="@string/sabah_sonu"
                    android:textColor="#121212"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="5dp"
                    android:textSize="@dimen/abc_text_size_medium_material" />
                <TextView
                    android:id="@+id/widgetSabahSonuVakit"
                    android:textColor="#121212"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="5dp"
                    android:gravity="end"
                    android:textSize="@dimen/abc_text_size_medium_material" />
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:id="@+id/ogleLinearLayout"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:paddingStart="20dip"
                android:paddingEnd="20dip">
                <TextView
                    android:id="@+id/widgetOgle"
                    android:text="@string/ogle"
                    android:textColor="#121212"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="5dp"
                    android:textSize="@dimen/abc_text_size_medium_material" />
                <TextView
                    android:id="@+id/widgetOgleVakti"
                    android:textColor="#121212"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="5dp"
                    android:gravity="end"
                    android:textSize="@dimen/abc_text_size_medium_material" />
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:id="@+id/ikindiLinearLayout"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:paddingEnd="20dip"
                android:paddingStart="20dip">
                <TextView
                    android:id="@+id/widgetIkindi"
                    android:text="@string/ikindi"
                    android:textColor="#121212"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="5dp"
                    android:textSize="@dimen/abc_text_size_medium_material" />
                <TextView
                    android:id="@+id/widgetIkindiVakit"
                    android:textColor="#121212"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="5dp"
                    android:gravity="end"
                    android:textSize="@dimen/abc_text_size_medium_material" />
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:id="@+id/aksamLinearLayout"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:paddingStart="20dip"
                android:paddingEnd="20dip">
                <TextView
                    android:id="@+id/widgetAksam"
                    android:text="@string/aksam"
                    android:textColor="#121212"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="5dp"
                    android:textSize="@dimen/abc_text_size_medium_material" />
                <TextView
                    android:id="@+id/widgetAksamVakit"
                    android:textColor="#121212"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="5dp"
                    android:gravity="end"
                    android:textSize="@dimen/abc_text_size_medium_material" />
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:id="@+id/yatsiLinearLayout"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:paddingEnd="20dip"
                android:paddingStart="20dip">
                <TextView
                    android:id="@+id/widgetYatsi"
                    android:text="@string/yatsi"
                    android:textColor="#121212"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="5dp"
                    android:textSize="@dimen/abc_text_size_medium_material" />
                <TextView
                    android:id="@+id/widgetYatsiVakit"
                    android:textColor="#121212"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="5dp"
                    android:gravity="end"
                    android:textSize="@dimen/abc_text_size_medium_material" />
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:id="@+id/yatsiSonuLinearLayout"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:paddingEnd="20dip"
                android:paddingStart="20dip">
                <TextView
                    android:id="@+id/widgetYatsiSonu"
                    android:text="@string/yatsi_sonu"
                    android:textColor="#121212"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="5dp"
                    android:textSize="@dimen/abc_text_size_medium_material" />
                <TextView
                    android:id="@+id/widgetYatsiSonuVakit"
                    android:textColor="#121212"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="5dp"
                    android:gravity="end"
                    android:textSize="@dimen/abc_text_size_medium_material" />
            </LinearLayout>
        </GridLayout>
    </FrameLayout>
</LinearLayout>